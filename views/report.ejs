<h3>ðŸ“‘ Report (Unique cars per day per camera)</h3>
<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>Date</th>
      <th>Camera</th>
      <th>Unique Plates</th>
    </tr>
  </thead>
  <tbody>
    <% if (stats.length === 0) { %>
      <tr><td colspan="3" class="text-center">No data</td></tr>
    <% } else { %>
      <% stats.forEach(s => { %>
        <tr>
          <td><%= s.day %></td>
          <td><%= s.cameraid %></td>
          <td><%= s.unique_plates %></td>
        </tr>
      <% }) %>
    <% } %>
  </tbody>
</table>
