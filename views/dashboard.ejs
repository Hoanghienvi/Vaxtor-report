<h3>ðŸ“Š Dashboard (last 50 records)</h3>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>ID</th>
      <th>Plate</th>
      <th>Country</th>
      <th>Confidence</th>
      <th>Proc. Time</th>
      <th>Camera</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% if (rows.length === 0) { %>
      <tr><td colspan="7" class="text-center">No data</td></tr>
    <% } else { %>
      <% rows.forEach(r => { %>
        <tr>
          <td><%= r.id %></td>
          <td><%= r.plate %></td>
          <td><%= r.country %></td>
          <td><%= r.confidence %></td>
          <td><%= r.processingtime %></td>
          <td><%= r.cameraid %></td>
          <td><%= r.date.toISOString().slice(0,19).replace("T"," ") %></td>
        </tr>
      <% }) %>
    <% } %>
  </tbody>
</table>
